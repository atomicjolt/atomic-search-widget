{
  "name": "react_client_starter_app",
  "version": "0.1.0",
  "description": "Client only React Starter App",
  "author": "atomicjolt",
  "license": "MIT",
  "homepage": "https://github.com/atomicjolt/react_client_starter_app",
  "scripts": {
    "build": "cd ./client && node ./build.js --release",
    "hot": "cd ./client && node webpack.hot.js",
    "lint": "cd ./client && eslint js",
    "release": "yarn build && node ./build.js --release && cd .. && ./client/node_modules/s3-website/s3-website.js deploy",
    "release_beta": "yarn build && ./client/node_modules/s3-website/s3-website.js deploy -f .s3-website.beta.json",
    "release_dev": "yarn build && ./client/node_modules/s3-website/s3-website.js deploy -f .s3-website.dev.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicjolt/atomic-search-widget"
  },
  "keywords": [],
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.2",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  }
}
